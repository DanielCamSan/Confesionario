<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp">
    <TextView
        android:id="@+id/ayudaTitleView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="40dp"
        android:paddingStart="?android:expandableListPreferredChildPaddingLeft"
        android:paddingLeft="?android:expandableListPreferredChildPaddingLeft"
        android:text="Title Example"
        android:textColor="@color/black"
        android:background="@color/ayuda_yellow"
        android:textSize="18dp"
        android:textAlignment="center"
        android:gravity="center"/>
</LinearLayout>